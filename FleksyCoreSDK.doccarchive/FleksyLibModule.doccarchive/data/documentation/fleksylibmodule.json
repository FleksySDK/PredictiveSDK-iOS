ference","isActive":true,"identifier":"doc:\/\/FleksyLibModule\/documentation\/FleksyLibModule\/LanguagesHelper"},{"type":"text","text":"."}],"identifier":"doc:\/\/FleksyLibModule\/documentation\/FleksyLibModule\/RemoteLanguage","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"RemoteLanguage"}],"url":"\/documentation\/fleksylibmodule\/remotelanguage"},"doc://FleksyLibModule/documentation/FleksyLibModule":{"role":"collection","title":"FleksyLibModule","abstract":[{"type":"text","text":"Also known as FleksyCoreSDK."}],"identifier":"doc:\/\/FleksyLibModule\/documentation\/FleksyLibModule","kind":"symbol","type":"topic","url":"\/documentation\/fleksylibmodule"},"doc://FleksyLibModule/documentation/FleksyLibModule/Replacement":{"role":"symbol","title":"Replacement","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Replacement"}],"abstract":[{"type":"text","text":"Contains information on how to make a replacement in an original text context."}],"identifier":"doc:\/\/FleksyLibModule\/documentation\/FleksyLibModule\/Replacement","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Replacement"}],"url":"\/documentation\/fleksylibmodule\/replacement"},"doc://FleksyLibModule/documentation/FleksyLibModule/LibraryConfiguration":{"role":"symbol","title":"LibraryConfiguration","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"LibraryConfiguration"}],"abstract":[{"type":"text","text":"The object required to initiualize and configure any "},{"type":"reference","isActive":true,"identifier":"doc:\/\/FleksyLibModule\/documentation\/FleksyLibModule\/FleksyLib"},{"type":"text","text":" instance."}],"identifier":"doc:\/\/FleksyLibModule\/documentation\/FleksyLibModule\/LibraryConfiguration","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"LibraryConfiguration"}],"url":"\/documentation\/fleksylibmodule